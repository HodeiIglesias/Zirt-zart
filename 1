import random
n = random.randint(1, 100)
for i in range(5):
    g = int(input("Adivina: "))
    if g == n: print("¡Correcto!"); break
    if i == 4: print("Perdiste. Era", n)
    elif i == 0: print("Está entre", n-20, "y", n+20)
    elif i == 1: print("Es", "mayor" if n > g else "menor")
    elif i == 2: print("Acaba en", n % 10)
    elif i == 3: print("Está entre", sorted([n]+random.sample([x for x in range(1,101) if x!=n],4)))
